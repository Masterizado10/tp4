    global FD
    print('Grabando todos los datos en el archivo ...')
    m = open("peajes-tp4.csv", 'r')
    peaje= m.read()


    m.close()

    oficial = open('peajes75.bin', "wb")
    pickle.dump(peaje,oficial)

    print('... hecho')
    print()

